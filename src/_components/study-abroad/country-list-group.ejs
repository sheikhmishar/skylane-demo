<div class="list-group d-md-block d-none" id="list-tab" role="tablist">
  <% db.countries_details.forEach(country => { %>
  <a
    class="list-group-item list-group-item-action p-3 <%- country.link ? "active" :"" %>"
    id="list-home-list"
    data-bs-toggle="lis"
    href="/study-abroad/country?country=<%= country.link %>"
    role="tab"
    aria-controls="list-home"
    >Study in <%= country.name %></a
  >
  <% }) %>   
</div>
