<div
  class="card card-visa align-items-center bg-transparent border-0 text-center"
>
  <img
    src="/assets/images/home/<%= eventsImg %>"
    class="card-img-top res-img"
    alt="<%= eventsAlt %>"
  />
  <div class="card-body">
    <h5 class="text-center choose-title h-25 mb-5 mb-lg-4">
      <%- eventsTitle %>
    </h5>
    <p class="text-center custom-para">
      <%= eventsDesc %>
    </p>
  </div>
  <div class="card-footer border-0 bg-transparent text-center">
    <svg
      xmlns="http://www.w3.org/2000/svg"
      width="20"
      height="20"
      fill="currentColor"
      class="bi bi-arrow-right read-more-text"
      viewBox="0 0 16 16"
    >
      <path
        fill-rule="evenodd"
        d="M1 8a.5.5 0 0 1 .5-.5h11.793l-3.147-3.146a.5.5 0 0 1 .708-.708l4 4a.5.5 0 0 1 0 .708l-4 4a.5.5 0 0 1-.708-.708L13.293 8.5H1.5A.5.5 0 0 1 1 8z"
      />
    </svg>
    <a href="<%= eventsLink %>" class="text-decoration-none read-more-text">Read More</a>
  </div>
</div>
