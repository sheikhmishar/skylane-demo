    <!-- contact form -->
    <section id="contact-form">
      <div class="container pt-5 pb-5">
        <div class="row">
          <div class="col-md-6" id="map">
            <div class="google-map">
              <iframe src="https://www.google.com/maps/d/embed?mid=1TViz5c8Likw3L_nvWo0JhqnLDGFchFI&ehbc=2E312F" width="100%" height="480"
                style="border: 0"
                allowfullscreen=""
                loading="lazy"
              ></iframe>             
            </div>
          </div>
          <div class="col-md-6" id="contact">
            <h4 class="custom-heading">Contact Us</h4>
            <h2 class="custom-title pb-2">
              Get in <em class="custom-em">touch</em>
            </h2>
            <div class="row">
              <div class="col-md-12">
                <div class="input-group input-button">
                  <p>
                    <span class="wpcf7-form-control-wrap" data-name="message">
                      <textarea
                        cols="200"
                        rows="4"
                        class="wpcf7-form-control wpcf7-textarea"
                        aria-invalid="false"
                        placeholder="Message"
                        name="message"
                      ></textarea>
                    </span>
                  </p>
                </div>
              </div>
              <div class="col-md-6">
                <div class="input-group">
                  <p>
                    <span class="wpcf7-form-control-wrap" data-name="your-name"
                      ><input
                        size="100"
                        class="wpcf7-form-control wpcf7-text"
                        aria-invalid="false"
                        placeholder="Name"
                        value=""
                        type="text"
                        name="your-name"
                    /></span>
                  </p>
                </div>
              </div>
              <div class="col-md-6">
                <div class="input-group">
                  <p>
                    <span class="wpcf7-form-control-wrap" data-name="your-email"
                      ><input
                        size="100"
                        class="wpcf7-form-control wpcf7-text"
                        aria-invalid="false"
                        placeholder="Email"
                        value=""
                        type="text"
                        name="your-email"
                    /></span>
                  </p>
                </div>
              </div>
              <div class="col-12 mt-3">
                <a type="submit" class="btn-about">Submit query</a>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
