  <script>
    var countriesJson = <%- JSON.stringify(db.countries_details) %>;
    var countryWithUniversityContent = <%- JSON.stringify(db.countries_universitiescontent) %>;
    var countryWithUniversity = <%- JSON.stringify(db.countries_universitiesdetails) %>;
    const urlParams = new URLSearchParams(window.location.search);
    const countryName = urlParams.get('country');
    const countryId = countriesJson.find(countryId => countryId.link === countryName);

    const filteredUniversityContent = countryWithUniversityContent.find(country => country.countries_detailId_ === countryId.id)
    const filteredUniversity = countryWithUniversity.filter(country => country.countries_detailId_ === countryId.id);
    const countryNameContainer = document.getElementById('countryname');
    const countryContentTitle = document.getElementById("countrycontent-title")
    const countryContentDescription = document.getElementById("countrycontent-description")

    const universityList = document.getElementById("universityList")

    if (filteredUniversity) {
      countryContentTitle.innerHTML = `
        <h4 class="custom-heading" id="countrycontent-title">Study in</h4>
              <h2 class="custom-title">
                <em class="custom-em">${filteredUniversityContent.title.slice(8,)}</em>
              </h2>    
        
        `
      countryContentDescription.innerHTML = filteredUniversityContent.content
      countryNameContainer.innerHTML = `${countryId.name} Universities`
      universityList.innerHTML = filteredUniversity.map(university =>

        `<div class="col ps-4 pe-4 ps-md-3 pe-md-3">
              <div class="card h-100 border-0 shadow">
                <div class="d-flex justify-content-center p-5 align-items-center">
                  <a href="/universities-we-represent/university?university=${university.link}"  rel="noopener noreferrer">
                    <img src="/${university.universityImgSrc}" class="img-fluid object-fit-cover" alt="${university.link}" />
                  </a>
                </div>
              </div>
            </div>`
      ).join(' ')
    }
  </script>
