<style>
  .card {
    position: relative;
    overflow: hidden;
  }

  .card:hover::before {
    left: 10%;
  }

  .card::before {
    position: absolute;
    content: "";
    width: 80%;
    height: 200%;
    background: #f47426;
    top: -50%;
    left: -100%;
    z-index: 1;
    transform: rotate(25deg);
    transform-origin: center top 0;
    transition: 0.5s;
    opacity: 1;
  }

  .card a {
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 3;
  }
</style>

