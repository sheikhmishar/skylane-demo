          <div class="card h-100 border-0">
            <div class="d-flex justify-content-center">
              <a href="/<%= gallery.imgSrc %>" class="popup">
                <img src="/<%= gallery.imgSrc %>" class="card-img-top" alt="photo gallery <%= gallery.text %>" />
              </a>
            </div>
            <div class="blog-details-content">
              <div class="card-body">
                <h5 class="blog-title"><%= gallery.text %></h5>
              </div>
            </div>
          </div>
        </div>
