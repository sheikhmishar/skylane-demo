  <div class="titlebar-wrapper" style="
    background-image: linear-gradient(
        rgb(2, 115, 154, 0.6),
        rgb(86, 198, 208, 0.5)
      ),
      url('/<%= db.photogallery_banner.imgSrc %>');
  ">
    <div class="titlebar-wrapper-header">
      <div class="container">
        <h1 class="text-white text-center black-italic"><%= db.photogallery_banner.bannerTitle %></h1>
      </div>
    </div>
  </div>

