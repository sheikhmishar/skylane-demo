<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <title>Our Sister Concerns - SkyLane</title>
  <%- include("/_components/head-seo") -%>
  <!--  -->
  <%- include("/_components/styles") -%>
</head>

<body>
  <%- include("/_components/nav") -%>

  <!-- sister concerns banner  -->
  <div class="titlebar-wrapper" style="
    background-image: linear-gradient(
      rgb(2, 115, 154, 0.6),
      rgb(86, 198, 208, 0.5)
      ),
      url('/<%= db.sisterconcerns_banner.imgSrc %>');
  ">
    <div class="titlebar-wrapper-header">
      <div class="container">
        <h1 class="text-white text-center black-italic"><%= db.sisterconcerns_banner.bannerTitle %></h1>
      </div>
    </div>
  </div>

  <!-- concerns -->
  <section class="pt-5 pb-5 mt-md-5 mb-md-5">
    <div class="container visa-conten">
      <div class="row row-cols-1 row-cols-md-2 g-4">
        <div class="col">
          <h4 class="custom-heading"><%= db.sisterconcerns_content.title %></h4>
          <h2 class="custom-title">
            We provide our Experts to
            <br />
            <em class="custom-em">Generate Great visa</em>
          </h2>
        </div>
        <div class="col">
          <p class="custom-paragraph">
            <%= db.sisterconcerns_content.text %>
          </p>
        </div>
      </div>

    </div>
  </section>

  <div class="container">
    <div class="row align-items-center" style="padding-bottom: 50px; border-bottom: 2px dotted #efefef">
      <% db.sisterconcerns_companies.forEach(company => { %>
      <% if (company.id === 1) { %>
      <div class="col-md-8">
        <h3 class="fw-bold" style="color: #02739a"><%= company.title %></h3>
        <p class="custom-para">
          <%= company.text %>
        </p>
        <div class="about-btn">
          <a href="#" class="btn-about">Visit now
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-arrow-right text-white" viewBox="0 0 16 16">
              <path fill-rule="evenodd" d="M1 8a.5.5 0 0 1 .5-.5h11.793l-3.147-3.146a.5.5 0 0 1 .708-.708l4 4a.5.5 0 0 1 0 .708l-4 4a.5.5 0 0 1-.708-.708L13.293 8.5H1.5A.5.5 0 0 1 1 8z"></path>
            </svg>
          </a>
        </div>
      </div>
      <div class="col-md-4">
        <div class="scholarship-img">
          <img src="/<%= company.imgSrc %>" alt="bhasha school logo" class="img-fluid" />
        </div>
      </div>
      <% } %>
      <% }) %>
    </div>
  </div>
  <div style="background: #f5f8fb">
    <div class="container">
      <div class="row align-items-center" style="
            padding-bottom: 50px;
            padding-top: 50px;
            border-bottom: 2px dotted #efefef;
          ">
        <% db.sisterconcerns_companies.forEach(company => { %>
        <% if (company.id === 2) { %>
        <div class="col-md-4 order-2 order-md-1">
          <div class="scholarship-img">
            <img src="/<%= company.imgSrc %>" alt="bookish pen logo" class="img-fluid" />
          </div>
        </div>
        <div class="col-md-8 order-1 order-md-2">
          <h3 class="fw-bold" style="color: #02739a"><%= company.title %></h3>

          <p class="custom-para">
            <%= company.text %>
          </p>
          <div class="about-btn">
            <a href="#" class="btn-about">Visit now
              <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-arrow-right text-white" viewBox="0 0 16 16">
                <path fill-rule="evenodd" d="M1 8a.5.5 0 0 1 .5-.5h11.793l-3.147-3.146a.5.5 0 0 1 .708-.708l4 4a.5.5 0 0 1 0 .708l-4 4a.5.5 0 0 1-.708-.708L13.293 8.5H1.5A.5.5 0 0 1 1 8z"></path>
              </svg>
            </a>
          </div>
        </div>
        <% } %>
        <% }) %>
      </div>
    </div>
  </div>
  <div class="container">
    <div class="row align-items-center" style="
          padding-bottom: 50px;
          padding-top: 50px;
        ">
      <% db.sisterconcerns_companies.forEach(company => { %>
      <% if (company.id === 3) { %>
      <div class="col-md-8">
        <h3 class="fw-bold" style="color: #02739a"><%= company.title %></h3>

        <p class="custom-para">
          <%= company.text %>
        </p>
        <div class="about-btn">
          <a href="#" class="btn-about">Visit now
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-arrow-right text-white" viewBox="0 0 16 16">
              <path fill-rule="evenodd" d="M1 8a.5.5 0 0 1 .5-.5h11.793l-3.147-3.146a.5.5 0 0 1 .708-.708l4 4a.5.5 0 0 1 0 .708l-4 4a.5.5 0 0 1-.708-.708L13.293 8.5H1.5A.5.5 0 0 1 1 8z"></path>
            </svg>
          </a>
        </div>
      </div>
      <div class="col-md-4 pt-5 pt-md-0">
        <div class="greenvalley-img text-center">
          <img src="/<%= company.imgSrc %>" alt="green valley logo" class="img-fluid" />
        </div>
      </div>
      <% } %>
      <% }) %>
    </div>
  </div>

  <%- include('/_components/footer') %> <%- include("/_components/scripts")
    -%>
</body>

</html>